            <kind>1</kind>
            <string>image_angle = direction

if (instance_exists (obj_server))
{
    var targetHero = scr_getTargetHero(id);
    var behaviour = scr_getNpcBehaviour(id, targetHero);
    switch(behaviour)
    {
        case "patrol":
            src_patrol();
            break;
        
        case "chase":
            var isChasing = scr_chaseHero(targetHero);
            if(isChasing == false) {
                src_patrol();
            }
            break;
        
        case "attack":
            scr_attackHero(targetHero);
            break;
            
        case "waiting":
            path_end();
            break;
    }
    scr_setSpritesNpc();
}
